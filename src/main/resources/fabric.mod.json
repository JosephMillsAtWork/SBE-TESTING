{
  "schemaVersion": 1,
  "id": "skyblockevolution",
  "version": "${version}",

  "name": "Skyblock Evolution 3.0",
  "description": "Skyblock Evolution 3.0 Port as a mod.",
  "authors": [
    "m_jimmer",
    "Dracconis"
  ],
  "contact": {
    "homepage": "https://skyblockevolution.com/",
    "sources": "https://github.com/JosephMillsAtWork/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/skyblockevolution/icon.png",
  "accessWidener": "skyblockevolution.accesswidener",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.skyblockevolution.SBE"
    ],
    "fabric-datagen": [
      "com.skyblockevolution._build.SBEDataGenerator"
    ],
    "modmenu": [
      "com.skyblockevolution.config.SBEModMenu"
    ]
  },
  "mixins": [
    "skyblockevolution.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
